{
  "name": "unused-assets-finder",
  "displayName": "Unused Assets Finder",
  "description": "Find and manage unused Asset files in your project.",
  "version": "0.0.3",
  "publisher": "shuheb",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/imShuheb/unusedFiles-extension-vs-code.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:unusedAssetsView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "unusedAssets",
          "title": "Assets",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "unusedAssets": [
        {
          "id": "unusedAssetsView",
          "name": "Assets"
        }
      ]
    },
    "commands": [
      {
        "command": "files.check",
        "title": "Refresh",
        "category": "Unused Files Tools",
        "icon": "$(refresh)"
      },
      {
        "command": "unusedAssets.previewFile",
        "title": "Preview",
        "category": "Unused Files Tools",
        "icon": "$(eye)"
      },
      {
        "command": "unusedAssets.deleteFile",
        "title": "Delete",
        "category": "Unused Files Tools",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "files.check",
          "group": "navigation",
          "icon": "$(refresh)"
        }
      ],
      "view/item/context": [
        {
          "command": "unusedAssets.previewFile",
          "group": "inline",
          "icon": "$(eye)"
        },
        {
          "command": "unusedAssets.deleteFile",
          "group": "inline",
          "icon": "$(trash)"
        }
      ],
      "explorer/context": [
        {
          "command": "unusedAssets.deleteFile",
          "when": "resourceExtname =~ /\\.(jpg|jpeg|png|gif|svg)$/",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.49.0",
    "esbuild": "^0.19.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "dependencies": {}
}
